# Tokopedia Voucher Claimer Configuration
# Copy this file to .env and fill in your credentials

# Tokopedia Account Credentials
TOKOPEDIA_EMAIL=your_email@example.com
TOKOPEDIA_PASSWORD=your_password

# Claimer Settings
CLAIM_DELAY_MIN=2
CLAIM_DELAY_MAX=5
MAX_RETRY_ATTEMPTS=3

# Browser Settings
HEADLESS_MODE=true
BROWSER_TIMEOUT=30

# Logging Settings
LOG_LEVEL=INFO
SAVE_REPORTS=true